/* ********** ON TOPIC ********** */

#on_topic {
    width: 158px;
    font: 12px Arial;
}

#on_topic table {
    width: 100%;
}

#on_topic h2 {
    color: #B41A02;
    font: bold 16px Arial;
    height: 22px;
    vertical-align: middle;
    margin: 0;
    padding: 2px 0 0 20px;
    text-decoration: none;
    background: url(../images/garnet_star.gif) no-repeat 0px;
}

#on_topic .on-topic-item {
    background: url(../images/box_interior.gif) repeat-y;
    font: 12px Arial;
    /* IE hack */
    height: 1%;
    line-height: 1.3;
    /* IE hack end */
    padding: 6px;
    margin: 0px;
}

#on_topic .on-topic-item a {
    color: #16348A;
    font: 13px Arial;
    text-decoration: none;
}

#on_topic .on-topic-item a:hover {
    text-decoration: underline;
}


