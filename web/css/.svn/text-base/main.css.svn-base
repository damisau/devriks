#container {
    position: relative;
    margin: auto;
    text-align: left;
    width: 950px;
}

#main_table {
    width: 100%;
}

#header {
    position: relative;
    width: 100%;
    height: 80px;
    background-color: white;
}

#logo {
    position: absolute;
    top: 2px;
    right: 20px;
    background-image: url(../images/logo.jpg);
    width: 100px;
    height: 52px;
    z-index: 10;
}

#map{
    width:600px;
    height:500px;
}

#title {
    position: absolute;
    top: 7px;
    left: 220px;
    font: bold 20px Arial;
    text-transform: uppercase;
    color: #662A42
}

#subtitle {
    position: absolute;
    top: 32px;
    left: 220px;
    font: bold 14px Arial;
    text-transform: uppercase;
    color: #662A42;
}

#crislogo{
    position: absolute;
    top: 2px;
    left: 20px;
    background-image: url(../images/crislogo.jpg);
    width: 145px;
    height: 60px;
    z-index: 1;
}

#top_menu_left {
    position: absolute;
    top: 59px;
    left: 0px;
    width: 3px;
    height: 25px;
    background: url(../images/topmenu_left.gif) no-repeat;
    z-index: 5;
}

#top_menu_bg {
    position: absolute;
    top: 59px;
    left: 3px;
    width: 944px;
    height: 25px;
    background: url(../images/topmenu_bg.gif) repeat-x;
    z-index: 5;
}

#top_menu_right {
    position: absolute;
    top: 59px;
    left: 947px;
    width: 3px;
    height: 25px;
    background: url(../images/topmenu_right.gif) no-repeat;
    z-index: 5;
}

#top_menu {
    position: absolute;
    top: 55px;
    left: 150px;
    vertical-align: top;
    z-index: 6;
}

#top_menu table {
    border: 0;
}

#top_menu .menu-lev1 {
